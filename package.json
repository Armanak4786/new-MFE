    "start:remotes": "npm-run-all --parallel start:dealer start:commercial start:admin",
    "start:dealer": "ng serve dealer",
    "start:commercial": "ng serve commercial",
    "start:admin": "ng serve admin",
    "start:all": "npm-run-all --parallel start:shell start:dealer start:commercial start:admin",
    "build": "ng build",
