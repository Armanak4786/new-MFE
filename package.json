    "build:dealer": "ng build dealer",
    "build:commercial": "ng build commercial",
    "build:admin": "ng build admin",
    "build:all": "npm-run-all --parallel build:authentication build:dealer build:commercial build:admin",
    "build:all:sequential": "npm-run-all build:authentication build:dealer build:commercial build:admin",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
